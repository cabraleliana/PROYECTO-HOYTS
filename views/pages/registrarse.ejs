<%-include('../partials/head')-%>

    <%-include('../partials/navbar')-%>

        <div>

            <div class="nav-login">
                <h2 class="login">CREAR USUARIO</h2>
            </div>

            <div class="d-flex container-login-general">
                <div class="d-flex div_uno ">
                    <div class="container-login">
                        <div class="mt-5 separar-divs">
                            <h8 class="bold">REGISTRATE Y CREA UN NUEVO USUARIO</h8>
                            <form>
                                <div class="form-group d-flex">
                                    <div class="separar-divs">
                                        <label for="exampleInputEmail1" class="bold mt-3">NOMBRE</label>
                                        <input name="name" class="form-control input no-hover" id="name" type="name"
                                            required="required" autocomplete="off">
                                    </div>
                                    <div class="separar-divs">
                                        <label for="exampleInputPassword1" class="bold_registrarse mt-3">APELLIDO</label>
                                        <input name="apellido" class="form-control input margin no-hover" id="apellido"
                                            type="text" required="required" autocomplete="off">
                                    </div>
                                </div>
                                <div class="form-group mt-3">
                                    <label for="exampleInputPassword1" class="bold">EMAIL</label>
                                    <input name="email" class="form-control input no-hover" id="email" type="email"
                                        required="required" autocomplete="off">
                                </div>
                                <div class="form-group d-flex">
                                    <div class="separar-divs">
                                        <label for="exampleInputEmail1" class="bold mt-3">CONTRASEÑA</label>
                                        <input name="password" class="form-control input no-hover" id="password" type="password"
                                            required="required" autocomplete="off">
                                    </div>
                                    <div class="separar-divs">
                                        <label for="exampleInputPassword1" class="bold_registrarse mt-3">CONFIRMAR
                                            CONTRASEÑA</label>
                                        <input name="password" class="form-control input margin no-hover" id="confirm_password"
                                            type="password" required="required" autocomplete="off">
                                    </div>
                                </div>
                                <div class="form-group d-flex">
                                    <div class="separar-divs">
                                        <label for="exampleInputEmail1" class="bold mt-3">FECHA DE NACIMIENTO</label>
                                        <input name="fecha_nacimiento" class="form-control input no-hover" id="fecha_nacimiento" type="date"
                                            required="required" autocomplete="off">
                                    </div>
                                        <div class="separar-divs">
                                            <label for="exampleInputPassword1" class="bold_registrarse mt-3">GENERO</label>
                                            <select name="select" id="genero" class="select_genero margin">
                                                <option value="">
                                                    SELECCIONE UNA OPCION
                                                </option>
                                                <option value="femenino">
                                                    FEMENINO
                                                </option>
                                                <option value="masculino">
                                                    MASCULINO
                                                </option>
                                            </select>
                                        </div>
                                </div>
                                <div class="form-group d-flex">
                                    <div class="separar-divs">
                                        <label for="exampleInputEmail1" class="bold mt-3">CELULAR</label>
                                        <input name="celular" class="form-control input no-hover" id="celular" type="celular"
                                            required="required" autocomplete="off">
                                    </div>
                                    <div class="separar-divs">
                                        <label for="exampleInputPassword1" class="bold_registrarse mt-3">COMPLEJO HABITUAL</label>
                                        <select name="select" id="complejo_habitual" class="select_genero margin">
                                            <option value="">
                                                SELECCIONE UNA OPCION
                                            </option>
                                            <option value="abasto">
                                                HOYTS ABASTO
                                            </option>
                                            <option value="dot">
                                                HOYTS DOT
                                            </option>
                                            <option value="moreno">
                                                HOYTS MORENO
                                            </option>
                                            <option value="quilmes">
                                                HOYTS QUILMES
                                            </option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
                                    <label class="form-check-label bold" for="flexCheckDefault">
                                        DESEO RECIBIR MAILS DE ESTRENO Y PROMOCIONES
                                    </label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked"
                                        checked>
                                    <label class="form-check-label bold" for="flexCheckChecked">
                                        ¿YA COMPRASTE EL PACK CINE FAN POR LAS BOLETERIAS DEL CINE?
                                    </label>
                                </div>

                            </form>
                            <div class="d-grid">

                                <button id="btn" type="button" class="mt-3 boton_registrarse bold"
                                    onclick="signIn()">REGISTRARSE</button>

                                <span id="usuarioExistente"></span>

                            </div>
                        </div>
                    </div>
                </div>


                <div class="promociones_benner_registrarse">

                    <img src="./img/banner.webp" alt="">

                </div>
            </div>

        </div>


        <script src="./js/login.js"></script>


        <%-include('../partials/footer')-%>